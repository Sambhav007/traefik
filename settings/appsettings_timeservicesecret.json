{
  
    "RabbitMQPublisher": {
      "Host": "rs-rmq-mgt.victoryautoworks.svc.cluster.local",
      "Username": "guest",
      "Password": "guest",
      "Exchange": "Pitstop"
    },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "TimeService"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://logserver:5341" }
      }
    ]
  }
}
